generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url = env("DB_URL")
}

model Trade {
  id String   @id @default(auto()) @map("_id") @db.ObjectId
  user_id String
  side String
  amount Float
  price Float
  tx_hash String
  timestamp DateTime @default(now())
}